<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="workflow_parent_access_view_tree" model="ir.ui.view">
        <field name="name">workflow.parent.access.tree</field>
        <field name="model">workflow.parent.access</field>
        <field name="arch" type="xml">
            <tree string="Access" editable="bottom">
                <field name="res_model" invisible="1"/>
