 <record id="project_budget_estimated_probability_kanban" model="ir.ui.view">
        <field name="name">project_budget.estimated_probability.kanban</field>
        <field name="model">project_budget.estimated_probability</field>
        <field name="arch" type="xml">
            <kanban class="oe_background_grey o_kanban_dashboard o_emphasize_colors o_stock_kanban" create="0" group_create="false">
                <field name="color"/>
                <field name="name" readonly="1"/>
                <field name="count_approve_state_total"/>
                <field name="sum_revenue_approve_state_total"/>
                <field name="sum_margin_income_total"/>
                <field name="sum_cost_price_total"/>
                <templates>
                    <t t-name="kanban-box">
                        <div t-if="['0','10', '30', '50','75','100','100(done)'].indexOf(record.name.raw_value) > -1" t-attf-class="#{!selection_mode ? kanban_color(record.color.raw_value) : ''}" name="estimated_probability">
                            <div t-attf-class="o_kanban_card_header">
                                <div class="o_kanban_card_header_title">
<!--                                    <div class="o_primary" t-if="['30', '50','75','100'].indexOf(record.name.raw_value) > -1">-->
<!--                                        <a>-->
<!--                                            probability-->
<!--                                            <field name="name"></field>-->
<!--                                        </a>-->
<!--                                    </div>-->

                                    <div class="o_primary" t-if="['0'].indexOf(record.name.raw_value) > -1">
                                        <a>
                                            Canceled, 0%
                                        </a>
                                    </div>
                                    <div class="o_primary" t-if="['10'].indexOf(record.name.raw_value) > -1">
                                        <a>
                                            Potential, 10%
                                        </a>
                                    </div>
                                    <div class="o_primary" t-if="['30'].indexOf(record.name.raw_value) > -1">
                                        <a>
                                            Opportunity, 30%
                                        </a>
                                    </div>
                                    <div class="o_primary" t-if="['50'].indexOf(record.name.raw_value) > -1">
                                        <a>
                                            Reserve, 50%
                                        </a>
                                    </div>
                                    <div class="o_primary" t-if="['75'].indexOf(record.name.raw_value) > -1">
                                        <a>
                                            Commitment, 75%
                                        </a>
                                    </div>
                                    <div class="o_primary" t-if="['100'].indexOf(record.name.raw_value) > -1">
                                        <a>
                                            Execution, 100%
                                        </a>
                                    </div>
                                    <div class="o_primary" t-if="['100(done)'].indexOf(record.name.raw_value) > -1">
                                        <a>
                                            Done, 100(done)
                                        </a>
                                    </div>
                                </div>
                                <div class="o_kanban_manage_button_section" t-if="!selection_mode">
                                    <a class="o_kanban_manage_toggle_button" href="#"><i class="fa fa-ellipsis-v" role="img" aria-label="Manage" title="Manage"/></a>
                                </div>
                            </div>
                            <div class="container o_kanban_card_content" t-if="!selection_mode">
                                <div class="row">
                                    <div t-if="record.name.raw_value == '0'" class="col-6 o_kanban_primary_left">
                                        <button class="btn btn-danger" name="get_action_projects_0" type="object">
                                            <span>Projects: <t t-esc="record.count_approve_state_total.value"/></span>
                                        </button>
                                    </div>
                                    <div t-if="record.name.raw_value == '10'" class="col-6 o_kanban_primary_left">
                                        <button class="btn btn-warning" name="get_action_projects_10" type="object">
                                            <span>Projects: <t t-esc="record.count_approve_state_total.value"/></span>
                                        </button>
                                    </div>

                                    <div t-if="record.name.raw_value == '30'" class="col-6 o_kanban_primary_left">
                                        <button class="btn btn-primary" name="get_action_projects_30" type="object">
                                            <span>Projects: <t t-esc="record.count_approve_state_total.value"/></span>
                                        </button>
                                    </div>
                                    <div t-if="record.name.raw_value == '50'" class="col-6 o_kanban_primary_left">
                                        <button class="btn btn-primary" name="get_action_projects_50" type="object">
                                            <span>Projects: <t t-esc="record.count_approve_state_total.value"/></span>
                                        </button>
                                    </div>
                                    <div t-if="record.name.raw_value == '75'" class="col-6 o_kanban_primary_left">
                                        <button class="btn btn-primary" name="get_action_projects_75" type="object">
                                            <span>Projects: <t t-esc="record.count_approve_state_total.value"/></span>
                                        </button>
                                    </div>
                                    <div t-if="record.name.raw_value == '100'" class="col-6 o_kanban_primary_left">
                                        <button class="btn btn-primary" name="get_action_projects_100" type="object">
                                            <span>Projects: <t t-esc="record.count_approve_state_total.value"/></span>
                                        </button>
                                    </div>
                                    <div t-if="record.name.raw_value == '100(done)'" class="col-6 o_kanban_primary_left">
                                        <button class="btn btn-success" name="get_action_projects_100done" type="object">
                                            <span>Projects: <t t-esc="record.count_approve_state_total.value"/></span>
                                        </button>
                                    </div>
                                    <div class="col-6 o_kanban_primary_right">
                                        <div class="col-12">
                                            <a>
                                                <field name="sum_revenue_approve_state_total"/>
                                                revenue
                                            </a>
                                        </div>
                                        <div class="col-12">
                                            <a>
                                                <field name="sum_cost_price_total"/>
                                                cost
                                            </a>
                                        </div>
                                        <div class="col-12">
                                            <a>
                                                <field name="sum_margin_income_total"/>
                                                margin
                                            </a>
                                        </div>
                                    </div>

                                </div>
                                <div class="container o_kanban_card_manage_pane dropdown-menu" role="menu">
                                    <div t-if="widget.editable" class="o_kanban_card_manage_settings row">
                                        <div class="col-8" role="menuitem" aria-haspopup="true">
                                            <ul class="oe_kanban_colorpicker" data-field="color" role="menu"/>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </t>
                </templates>
            </kanban>
        </field>
    </record>
    </data>
