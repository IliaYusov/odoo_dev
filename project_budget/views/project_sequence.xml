<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data noupdate="1">
        <record id="project_id_generate" model="ir.sequence">
            <field name="name">Project ID</field>
            <field name="code">project_budget.projects</field>
            <field name="prefix">PRJ-%(year)s%(month)s-</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <record id="step_id_generate" model="ir.sequence">
            <field name="name">Step ID</field>
            <field name="code">project_budget.project_steps</field>
            <field name="prefix">%(doy)s-</field>
            <field name="padding">3</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <record id="acceptance_id_generate" model="ir.sequence">
            <field name="name">Acceptance ID</field>
            <field name="code">project_budget.planned_acceptance_flow</field>
            <field name="prefix">%(year)s%(month)s-</field>
            <field name="padding">6</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <record id="cash_id_generate" model="ir.sequence">
            <field name="name">Cash ID</field>
            <field name="code">project_budget.planned_cash_flow</field>
            <field name="prefix">%(year)s%(month)s-</field>
            <field name="padding">6</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <record id="tender_id_generate" model="ir.sequence">
            <field name="name">Tender ID</field>
            <field name="code">project_budget.tenders</field>
            <field name="prefix">TND-%(year)s%(month)s-</field>
            <field name="padding">6</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

    </data>
</odoo>